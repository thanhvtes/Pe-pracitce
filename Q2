#include<stdio.h>
#include<string.h>

int main()
{
    char str[1000];
    int digit = 0;
    int i = 0;
    scanf ("%[^\n]", str);
    while (str[i] != '\0'){
        if((str[i]>='a' && str[i]<='z') || (str[i]>='A' && str[i]<='Z'))
            digit++;
        i++;
    }
    printf("Number of digit character in the string is : %d\n", digit);
    return 0;
}
